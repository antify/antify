{
  "name": "@antify/antify-admin",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "nuxi build",
    "dev": "nuxi dev",
    "generate": "nuxi generate",
    "preview": "nuxi preview",
    "app:make:migration": "npx prisma migrate dev --schema=./server/datasources/db/schema.prisma --name",
    "app:db:reinit": "npx prisma migrate reset --force --preview-feature --schema=./server/datasources/db/schema.prisma && npx prisma migrate deploy --preview-feature --schema=./server/datasources/db/schema.prisma",
    "app:rebuild": "pnpm app:db:reinit && pnpm app:db:install && npx ts-node ./command/fixtures/fixtures.ts",
    "app:reinit": "rm -rf server/datasources/db/migrations && pnpm app:make:migration init",
    "app:db:install": "npx ts-node ./command/install/index.ts"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.2.0",
    "@nuxt/kit": "3.0.0-rc.4",
    "@nuxtjs/tailwindcss": "^5.0.3",
    "autoprefixer": "^10.4.4",
    "postcss": "^8.4.12",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@antify/ant-validate": "1.0.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "uuid": "^8.3.2",
    "@prisma/client": "^3.14.0",
    "nuxt": "3.0.0-rc.4",
    "prisma": "^3.14.0"
  }
}
